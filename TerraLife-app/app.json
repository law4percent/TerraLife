{
  "expo": {
    "name": "TerraLife",
    "slug": "firebase-auth2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/loading-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.law4percent.firebaseauth2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b455ad4-af8b-4c70-a80a-9d4319807432"
      }
    }
  }
}
